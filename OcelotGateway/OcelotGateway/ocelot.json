{
    "Routes": [
        {
            "DownstreamPathTemplate": "/product-api/{everything}",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "host.docker.internal",
                    "Port": 5001
                }
            ],
            "UpstreamHttpMethod": [ "Get" ],
            "UpstreamPathTemplate": "/product-gateway/{everything}",
            "RateLimitOptions": {
                "ClientWhitelist": [],
                "EnableRateLimiting": true,
                "Period": "1m",
                "PeriodTimespan": 60,
                "Limit": 10
            },
            "SwaggerKey": "api1"
        },
        {
            "DownstreamPathTemplate": "/cart-api/{everything}",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "host.docker.internal",
                    "Port": 6001
                }
            ],
            "UpstreamHttpMethod": [ "Get" ],
            "UpstreamPathTemplate": "/cart-gateway/{everything}",
            "RateLimitOptions": {
                "ClientWhitelist": [],
                "EnableRateLimiting": true,
                "Period": "1m",
                "PeriodTimespan": 60,
                "Limit": 10
            },
            "SwaggerKey": "api2"
        }
    ],

    "SwaggerEndPoints": [
        {
            "Key": "api1",
            "Config": [
                {
                    "Name": "API product",
                    "Version": "v1",
                    "Url": "http://host.docker.internal:5001/swagger/v1/swagger.json"
                }
            ]
        },
        {
            "Key": "api2",
            "Config": [
                {
                    "Name": "API cart",
                    "Version": "v1",
                    "Url": "http://host.docker.internal:6001/swagger/v1/swagger.json"
                }
            ]
        }
    ],
    "GlobalConfiguration": {
        "BaseUrl": "http://localhost:7001"
    }
}